{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Frontend/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/Frontend/build/static/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/Frontend/build/images/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/Frontend/build/manifest.json"
    },
    {
      "src": "/Development.pdf",
      "dest": "/Frontend/build/Development.pdf"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/build/index.html"
    }
  ]
}
